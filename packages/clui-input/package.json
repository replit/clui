{
  "name": "@replit/clui-input",
  "version": "0.0.15",
  "description": "A utility library for building CLI style interfaces with autocomplete",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm run test && npm run build",
    "typedoc:build": "typedoc src",
    "typedoc:serve": "npm run docs:build && npx serve docs",
    "test": "npm-run-all --parallel test:*",
    "test:format": "prettier --check \"src/**/*.ts\"",
    "test:lint": "eslint src/ --ext .ts",
    "test:tsc": "tsc",
    "test:unit": "jest"
  },
  "author": "moudy@repl.it",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "events": "^3.1.0",
    "html-colors": "0.0.6",
    "husky": "^4.2.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typedoc": "^0.16.8",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "repository": "git@github.com:replit/clui.git"
}
